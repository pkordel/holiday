<% @errors.each do |error| %>
  <%= error %><br/>
<% end %>

<%= form_for @holiday_request do |f| %>
  Employee: <%= f.select :employee_id, @employees.map {|employee| [employee.email, employee.id]} %>
  <br/>
  Starts on: <%= f.date_select :starts_on %>
  <br/>
  Ends on: <%= f.date_select :ends_on %>
  <br/>
  <%= f.submit %> or <%= link_to "Cancel", holiday_requests_path %>
<% end %>